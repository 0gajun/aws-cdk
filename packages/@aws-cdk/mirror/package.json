{
  "name": "@aws-cdk/mirror",
  "version": "0.25.3",
  "description": "Look how pretty your CDK app is!",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.mirror",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "cdk-mirror"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.Mirror",
        "packageId": "Amazon.CDK.AWS.Mirror",
        "signAssembly": true,
        "assemblyOriginatorKeyFile": "../../key.snk"
      },
      "sphinx": {}
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/aws-cdk.git"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "integ": "cdk-integ",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "statements": 80
      }
    },
    "moduleFileExtensions": [ "js" ]
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "mirror"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "cdk-build-tools": "^0.25.3",
    "cdk-integ-tools": "^0.25.3",
    "pkglint": "^0.25.3"
  },
  "dependencies": {
    "@aws-cdk/cdk": "^0.25.3"
  },
  "homepage": "https://github.com/awslabs/aws-cdk",
  "peerDependencies": {
    "@aws-cdk/cdk": "^0.25.3"
  },
  "engines": {
    "node": ">= 8.10.0"
  }
}
